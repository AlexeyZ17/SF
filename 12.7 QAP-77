money = int(input("Введите сумму, которую планируете положить под проценты"))
per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
n = money/100
T = int((per_cent['ТКБ']) * n)
S = int((per_cent['СКБ']) * n)
V = int((per_cent['ВТБ']) * n)
SB = int((per_cent['СБЕР']) * n)
deposit = [T, S, V, SB]
print("Накопленные средства за год вклада:", deposit)
print("Максимальная сумма, которую Вы заработаете:", max(deposit))
